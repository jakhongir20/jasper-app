{
  "name": "react-ts-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "generate:icon-type": "node generate-icon-types.js",
    "generate:api": "bunx orval --config ./orval.config.ts",
    "generate:api:watch": "bunx orval --config ./orval.config.ts --watch",
    "build": "bun run generate:api && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint --ext .js,.cjs,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.cjs,.ts,.tsx . --fix",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@tanstack/react-query": "^5.64.2",
    "antd": "^5.23.1",
    "antd-style": "^3.7.1",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "google-libphonenumber": "^3.2.40",
    "i18next": "^24.2.1",
    "maplibre-gl": "^5.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "react-international-phone": "^4.5.0",
    "react-number-format": "^5.4.3",
    "react-router-dom": "^7.0.2",
    "react-transition-group": "^4.4.5",
    "style-loader": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-svgr": "^4.3.0"
  },
  "devDependencies": {
    "@orval/core": "^7.11.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/lodash": "^4.17.20",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-transition-group": "^4.4.12",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^25.0.1",
    "lodash": "^4.17.21",
    "orval": "^7.11.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "vite": "^5.4.8"
  },
  "license": "MIT"
}
